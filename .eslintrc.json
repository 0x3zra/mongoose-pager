{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true }],
        "no-console": 0,
        "no-empty": 0,
        "no-irregular-whitespace": 0
    }
}
